<div class="flex text-inherit justify-end items-center text-lg">
	<div
		class="transition-all text-inherit container items-center justify-between gap-x-5 lg:gap-x-10 mx-5 lg:mx-10 text-xs lg:text-lg hidden lg:flex">
		<% endpoints.forEach((endpoint) => { %>
		<a
			href="<%= endpoint.path %>"
			class="transition-all hover:text-stone-400">
			<%= endpoint.name %>
		</a>
		<% }); %>
	</div>
	<div class="flex justify-self-end gap-x-5 items-center">
		<a href="/bag">
			<svg
				xmlns="http://www.w3.org/2000/svg"
				fill="none"
				viewBox="0 0 24 24"
				stroke-width="1.5"
				stroke="currentColor"
				class="w-6 h-6">
				<path
					stroke-linecap="round"
					stroke-linejoin="round"
					d="M15.75 10.5V6a3.75 3.75 0 10-7.5 0v4.5m11.356-1.993l1.263 12c.07.665-.45 1.243-1.119 1.243H4.25a1.125 1.125 0 01-1.12-1.243l1.264-12A1.125 1.125 0 015.513 7.5h12.974c.576 0 1.059.435 1.119 1.007zM8.625 10.5a.375.375 0 11-.75 0 .375.375 0 01.75 0zm7.5 0a.375.375 0 11-.75 0 .375.375 0 01.75 0z" />
			</svg>
		</a>
		<% if(user) { %>
		<button
			class="hidden lg:block"
			type="button"
			id="accountDropDown"
			data-te-dropdown-toggle-ref
			aria-expanded="false">
			<svg
				xmlns="http://www.w3.org/2000/svg"
				fill="none"
				viewBox="0 0 24 24"
				stroke-width="1.5"
				stroke="currentColor"
				class="w-6 h-6">
				<path
					stroke-linecap="round"
					stroke-linejoin="round"
					d="M17.982 18.725A7.488 7.488 0 0012 15.75a7.488 7.488 0 00-5.982 2.975m11.963 0a9 9 0 10-11.963 0m11.963 0A8.966 8.966 0 0112 21a8.966 8.966 0 01-5.982-2.275M15 9.75a3 3 0 11-6 0 3 3 0 016 0z" />
			</svg>
		</button>
		<ul
			class="absolute z-[1000] m-0 hidden min-w-max list-none overflow-hidden border-none bg-stone-50 text-left text-base shadow-md [&[data-te-dropdown-show]]:block"
			aria-labelledby="accountDropDown"
			data-te-dropdown-menu-ref>
			<h6
				class="block w-full whitespace-nowrap bg-transparent py-2 px-4 text-sm font-semibold text-neutral-900">
				<%= user.username %>
			</h6>
			<li>
				<a
					class="block w-full whitespace-nowrap bg-transparent py-2 px-4 text-sm font-normal text-neutral-700 hover:bg-neutral-100 active:text-neutral-800 active:no-underline disabled:pointer-events-none disabled:bg-transparent disabled:text-neutral-400 dark:hover:bg-neutral-600"
					href="/profile"
					data-te-dropdown-item-ref
					>My Account</a
				>
			</li>
			<% if(user.role > 0) { %>
			<li>
				<a
					class="block w-full whitespace-nowrap bg-transparent py-2 px-4 text-sm font-normal text-neutral-700 hover:bg-neutral-100 active:text-neutral-800 active:no-underline disabled:pointer-events-none disabled:bg-transparent disabled:text-neutral-400 dark:hover:bg-neutral-600"
					href="/restaurant"
					data-te-dropdown-item-ref
					>Restaurant</a
				>
			</li>
			<% } %>
			<li>
				<a
					class="block w-full whitespace-nowrap bg-transparent py-2 px-4 text-sm font-normal text-neutral-700 hover:bg-neutral-100 active:text-neutral-800 active:no-underline disabled:pointer-events-none disabled:bg-transparent disabled:text-neutral-400 dark:hover:bg-neutral-600"
					href="/logout"
					data-te-dropdown-item-ref
					>Logout</a
				>
			</li>
		</ul>
		<% } else { %>
		<a href="/login" class="hidden lg:block">
			<svg
				xmlns="http://www.w3.org/2000/svg"
				fill="none"
				viewBox="0 0 24 24"
				stroke-width="1.5"
				stroke="currentColor"
				class="w-6 h-6">
				<path
					stroke-linecap="round"
					stroke-linejoin="round"
					d="M17.982 18.725A7.488 7.488 0 0012 15.75a7.488 7.488 0 00-5.982 2.975m11.963 0a9 9 0 10-11.963 0m11.963 0A8.966 8.966 0 0112 21a8.966 8.966 0 01-5.982-2.275M15 9.75a3 3 0 11-6 0 3 3 0 016 0z" />
			</svg>
		</a>
		<% } %>
		<button
			class="group peer block lg:hidden"
			type="button"
			data-te-collapse-init
			data-te-target="#cart"
			aria-expanded="false"
			aria-controls="#cart">
			<svg
				xmlns="http://www.w3.org/2000/svg"
				fill="none"
				viewBox="0 0 24 16"
				stroke-width="1.5"
				stroke="currentColor"
				class="w-6 h-6 transition-all hover:brightness-75 group-aria-expanded:rotate-90">
				<path
					stroke-linecap="round"
					stroke-linejoin="round"
					d="M3.75 6.75h16.5M3.75 12h16.5" />
			</svg>
		</button>
		<div
			class="-z-20 transition-all w-full h-full bg-black opacity-0 fixed invisible peer-aria-expanded:visible peer-aria-expanded:opacity-50 top-0 left-0"></div>
	</div>
</div>
